        function TD(unix_time_sec) {
            var d = new Date(unix_time_sec*1000)
            var tzs, tz = d.getTimezoneOffset()/-60
            if(localStorage.myTZ > -13 && localStorage.myTZ < 13) tz = Number(localStorage.myTZ)
            if(tz<0) tzs = 'GMT'+tz
            else if(tz>0) tzs = 'GMT+'+tz
            else tzs = 'UTC'
            d = new Date(d.getTime() + 3600000*tz)
            document.write(d.toISOString().replace('T',' ').replace('.000','').replace('Z','')+' ('+tzs+')')
